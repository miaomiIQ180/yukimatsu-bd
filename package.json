{
  "name": "yukimatsu-bd",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.1",
  "description": "A nice template for starting a vue-ts project ",
  "author": "Oliver Mak",
  "license": "MIT",
  "homepage": "https://miaomiIQ180.github.io/yukimatsu-bd",
  "repository": {
    "type": "git",
    "url": "https://github.com/miaomiIQ180/yukimatsu-bd"
  },
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "vite",
    "prepare": "simple-git-hooks && nr build-unplugin",
    "build": "run-p \"build-only {@}\" type-check --",
    "build-only": "vite build",
    "build-unplugin": "nr build-only && rimraf ./dist",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --cache --fix",
    "lint:check": "eslint . --cache",
    "lint:inspect": "eslint --inspect-config",
    "release": "bumpp"
  },
  "dependencies": {
    "@vueuse/core": "^12.0.0",
    "v-viewer": "^3.0.21",
    "vue": "^3.5.13",
    "vue-confetti-explosion": "^1.0.2"
  },
  "devDependencies": {
    "@antfu/ni": "^0.23.1",
    "@iconify/vue": "^4.2.0",
    "@oliver139/eslint-config": "^1.1.3",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "bumpp": "^9.9.1",
    "eslint": "^9.17.0",
    "lint-staged": "^15.2.11",
    "npm-run-all2": "^7.0.2",
    "rimraf": "^6.0.1",
    "sass": "^1.83.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "~5.7.2",
    "unplugin-auto-import": "^0.19.0",
    "unplugin-vue-components": "^0.28.0",
    "vite": "^6.0.5",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vue-tsc": "^2.1.10"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --cache --fix"
  },
  "browserslist": [
    ">0.3%",
    "defaults"
  ]
}
